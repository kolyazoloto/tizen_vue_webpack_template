      "tags": [
        {
          "name": "vue-loaders",
          "description": "Single component for all of the loaders. Use `name` attribute to specify loader.",
          "source": {
            "module": "vue-loaders",
            "symbol": "default"
          },
          "doc-url": "https://github.com/Hokid/vue-loaders",
          "attributes": [
            {
              "name": "name",
              "description": "Set loader name to show.\n\nAvailable names:\n - ball-beat\n - ball-clip-rotate-multiple\n - ball-clip-rotate-pulse\n - ball-clip-rotate\n - ball-grid-beat\n - ball-grid-pulse\n - ball-pulse-rise\n - ball-pulse-sync\n - ball-pulse\n - ball-rotate\n - ball-scale-multiple\n - ball-scale-ripple-multiple\n - ball-scale-ripple\n - ball-scale\n - ball-spin-fade-loader\n - ball-triangle-path\n - ball-zig-zag-deflect\n - ball-zig-zag\n - cube-transition\n - line-scale-party\n - line-scale-pulse-out-rapid\n - line-scale-pulse-out\n - line-scale\n - line-spin-fade-loader\n - pacman\n - semi-circle-spin\n - square-spin\n - triangle-skew-spin",
              "value": {
                "kind": "expression",
                "type": "string"
              },
              "required": true
            },
            {
              "name": "color",
              "description": "Set color. Accepted any CSS color value.",
              "value": {
                "kind": "expression",
                "type": "string"
              },
              "required": false,
              "default": "\"#ffffff\""
            },
            {
              "name": "scale",
              "description": "Set scale. Accepted any CSS number value.",
              "value": {
                "kind": "expression",
                "type": [
                  "string",
                  "number"
                ]
              },
              "required": false
            }
          ]
